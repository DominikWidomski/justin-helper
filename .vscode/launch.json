{
        "version": "0.2.0",
        "configurations": [
            {
                "type": "node",
                "request": "launch",
                "console": "integratedTerminal",
                "name": "Launch Taskr",
                "program": "${workspaceRoot}/node_modules/.bin/taskr"
            },

            {
                "type": "node",
                "request": "launch",
                "console": "integratedTerminal",
                "name": "Launch DEV",
                "program": "${workspaceRoot}/build/index",
                "sourceMaps": true,
                "outFiles": [
                    "${workspaceRoot}/build/**/*"
                ],
