                // "smartStep": true
            },

            {
                "type": "node",
                "request": "launch",
                "console": "integratedTerminal",
                "name": "Launch TEST",
                "program": "${workspaceRoot}/node_modules/.bin/jest",
                "args": [
                    "--testNamePattern='getNextActionParams'"
                ],
                "sourceMaps": true,
                "outFiles": [
                    "${workspaceRoot}/build/*"
                ],
                "sourceMapPathOverrides": {
                    "*": "${workspaceRoot}/src/*",
